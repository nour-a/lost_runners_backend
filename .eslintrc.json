{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "plugins": [
  ],
  "rules": {
    "no-console": "warn",
    "indent":[],
    "space-before-blocks": "warn",
    "arrow-spacing": "warn",
    "keyword-spacing": "warn",
    "space-infix-ops": "warn",
    "space-in-parens": "warn",
    "spaced-comment": "warn",
    "space-before-function-paren": "warn",
    "semi": "warn",
    "quotes": ["error", "single"],
    "object-curly-spacing": "warn",
    "no-multiple-empty-lines": "warn"
  }
}